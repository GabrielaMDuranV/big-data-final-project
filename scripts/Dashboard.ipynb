   "source": [
    "# dashboard_edades.py\n",
    "# Dashboard interactivo para \"Edades1_filtrado.csv\"\n",
    "# Requerimientos: pandas, plotly, dash, dash_bootstrap_components\n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import plotly.express as px\n",
    "import plotly.graph_objects as go\n",
    "\n",
    "# Opcional: suprimir algunos warnings molestos (FutureWarning de pandas/plotly)\n",
    "import warnings\n",
    "warnings.filterwarnings(\"ignore\", category=FutureWarning)\n",
    "\n",
    "\n",
    "from dash import Dash, dcc, html, Input, Output\n",
    "import dash_bootstrap_components as dbc\n",
    "\n",
    "# ---------------------------\n",
    "# 1) CONFIG: nombres de archivo y mapeos\n",
    "# ---------------------------\n",
    "CSV_PATH = \"Edades1_filtrado.csv\"  # debe estar en la misma carpeta que este script\n",
    "\n",
    "# Mapeos proporcionados por ti:\n",
    "map_sex = {1: \"Femenino\", 2: \"Masculino\"}\n",
    "map_school_type = {1: \"Pública\", 2: \"Privada\", 3: \"Convenio\"}\n",
    "map_resdept = {\n",
    "    2.0: \"La Paz\",\n",
    "    7.0: \"Santa Cruz\",\n",
    "    3.0: \"Cochabamba\",\n",
    "    5.0: \"Potosí\",\n",
    "    1.0: \"Chuquisaca (Sucre)\",\n",
    "    4.0: \"Oruro\",\n",
    "    6.0: \"El Alto\",\n",
    "    8.0: \"Tarija\",\n",
    "    9.0: \"Beni\",\n",
    "    10.0: \"Pando\"\n",
    "}\n",
    "\n",
    "# Cantidad de unidades educativas por departamento (valores que nos diste)\n",
    "schools_per_department = {\n",
    "    \"El Alto\": 524,\n",
    "    \"La Paz\": 4230,\n",
    "    \"Cochabamba\": 3700,\n",
    "    \"Santa Cruz\": 3400,\n",
    "    \"Potosí\": 1500,\n",
    "    \"Chuquisaca (Sucre)\": 1050,\n",
    "    \"Oruro\": 850,\n",
    "    \"Tarija\": 800,\n",
    "    \"Beni\": 700,\n",
    "    \"Pando\": 250\n",
    "}\n",
    "\n",
    "# Mapas para niveles educativos (BO2012A_EDLEV)\n",
    "map_edlev = {\n",
    "    3: \"Inicial (pre-kinder/kinder)\",\n",
    "    4: \"Básico (1-5 años) [sistema anterior]\",\n",
    "    5: \"Intermedio (6-8 años) [sistema anterior]\",\n",
    "    6: \"Medio (9-12 años) [sistema anterior]\",\n",
    "    7: \"Primaria (1-8 años)\",\n",
    "    8: \"Secundaria (9-12 años) [sistema actual]\",\n",
    "    9: \"Primaria actual (1-6 años)\",\n",
    "    10: \"Secundaria actual (7-12 años)\"\n",
    "}\n",
    "\n",
    "# Función para agrupar edades en categorías\n",
    "def age_group_label(age):\n",
    "    try:\n",
    "        a = float(age)\n",
    "    except:\n",
    "        return \"Sin dato\"\n",
    "    if a >= 0 and a <= 5:\n",
    "        return \"Primera infancia (0-5)\"\n",
    "    if 6 <= a <= 11:\n",
    "        return \"Niños/as (6-11)\"\n",
    "    if 12 <= a <= 17:\n",
    "        return \"Adolescentes (12-17)\"\n",
    "    if 18 <= a <= 28:\n",
    "        return \"Jóvenes (18-28)\"\n",
    "    if a >= 29:\n",
    "        return \"29+\"\n",
    "    return \"Sin dato\"\n",
    "\n",
    "# ---------------------------\n",
    "# 2) CARGA Y LIMPIEZA\n",
    "# ---------------------------\n",
    "df = pd.read_csv(CSV_PATH)\n",
    "\n",
    "# Renombrar columnas (asegurar existencia)\n",
    "# (Si tu CSV ya tiene exactamente estos nombres no es necesario, pero lo dejamos por seguridad)\n",
    "# Columnas clave: BO2012A_SCHOOL, BO2012A_AGE, BO2012A_SEX, BO2012A_EDLEV, BO2012A_RESDEPT\n",
    "for col in [\"BO2012A_SCHOOL\", \"BO2012A_AGE\", \"BO2012A_SEX\", \"BO2012A_EDLEV\", \"BO2012A_RESDEPT\"]:\n",
    "    if col not in df.columns:\n",
    "        raise ValueError(f\"El archivo no contiene la columna esperada: {col}\")\n",
    "\n",
    "# Quitar filas donde age o sex o resdept sean NaN (para análisis principales)\n",
    "df_clean = df.copy()\n",
    "\n",
    "# Mapear códigos a etiquetas legibles\n",
    "df_clean[\"sex_label\"] = df_clean[\"BO2012A_SEX\"].map(map_sex).fillna(\"Desconocido\")\n",
    "df_clean[\"school_type_label\"] = df_clean[\"BO2012A_SCHOOL\"].map(map_school_type).fillna(\"Desconocido\")\n",
    "df_clean[\"dept_label\"] = df_clean[\"BO2012A_RESDEPT\"].map(map_resdept).fillna(\"Desconocido\")\n",
    "df_clean[\"edlev_label\"] = df_clean[\"BO2012A_EDLEV\"].map(map_edlev).fillna(\"Otro/Desconocido\")\n",
    "\n",
    "# Edad: crear columna numérica y categoría\n",
    "df_clean[\"age_num\"] = pd.to_numeric(df_clean[\"BO2012A_AGE\"], errors=\"coerce\")\n",
    "df_clean[\"age_group\"] = df_clean[\"age_num\"].apply(lambda x: age_group_label(x) if not pd.isna(x) else \"Sin dato\")\n",
    "\n",
    "# Contar valores faltantes para informar\n",
    "missing_summary = df_clean[[\"BO2012A_AGE\",\"BO2012A_SEX\",\"BO2012A_SCHOOL\",\"BO2012A_EDLEV\",\"BO2012A_RESDEPT\"]].isna().sum().to_dict()\n",
    "\n",
    "# ---------------------------\n",
    "# 3) METRICAS y TABLAS DERIVADAS\n",
    "# ---------------------------\n",
    "# Totales básicos\n",
    "total_students = len(df_clean)\n",
    "total_by_sex = df_clean[\"sex_label\"].value_counts().to_dict()\n",
    "total_by_dept = df_clean[\"dept_label\"].value_counts().to_dict()\n",
    "total_by_school_type = df_clean[\"school_type_label\"].value_counts().to_dict()\n",
    "\n",
    "# Para cada departamento: total estudiantes y estudiantes por colegio (usando schools_per_department)\n",
    "dept_list = list(schools_per_department.keys())\n",
    "\n",
    "# Preparar DataFrame resumen por departamento (incluye departamentos con 0 estudiantes si aplica)\n",
    "dept_summary = pd.DataFrame({\n",
    "    \"department\": dept_list,\n",
    "    \"school_units\": [schools_per_department.get(d, np.nan) for d in dept_list]\n",
    "})\n",
    "\n",
    "# Contar estudiantes por departamento en nuestros datos\n",
    "students_by_dept = df_clean.groupby(\"dept_label\").size().reset_index(name=\"students_count\")\n",
    "\n",
    "# Unir con dept_summary (puede haber departamentos en datos que no estén en schools_per_department)\n",
    "dept_summary = dept_summary.merge(students_by_dept, left_on=\"department\", right_on=\"dept_label\", how=\"left\").drop(columns=[\"dept_label\"])\n",
    "dept_summary[\"students_count\"] = dept_summary[\"students_count\"].fillna(0).astype(int)\n",
    "\n",
    "# Calcular estudiantes por colegio (students_count / school_units)\n",
    "dept_summary[\"students_per_school\"] = dept_summary.apply(\n",
    "    lambda row: (row[\"students_count\"] / row[\"school_units\"]) if row[\"school_units\"] and row[\"school_units\"]>0 else np.nan,\n",
    "    axis=1\n",
    ")\n",
    "\n",
    "# Estadísticas globales (promedio, mediana) del número de estudiantes por departamento (de los datos)\n",
    "mean_students_per_dept = dept_summary[\"students_count\"].mean()\n",
    "median_students_per_dept = dept_summary[\"students_count\"].median()\n",
    "\n",
    "# Estadísticas por sexo y por departamento\n",
    "students_by_dept_sex = df_clean.groupby([\"dept_label\",\"sex_label\"]).size().reset_index(name=\"count\")\n",
    "\n",
    "# Estadísticas por tipo de institución\n",
    "students_by_school_type = df_clean.groupby(\"school_type_label\").size().reset_index(name=\"count\")\n",
    "\n",
    "# Distribución por grupos etarios (age_group)\n",
    "age_group_counts = df_clean[\"age_group\"].value_counts().reset_index()\n",
    "age_group_counts.columns = [\"age_group\",\"count\"]\n",
    "\n",
    "# Algunas métricas adicionales a mostrar en KPIs:\n",
    "total_schools = sum(schools_per_department.values())\n",
    "avg_students_per_school_overall = total_students / total_schools if total_schools>0 else np.nan\n",
    "\n",
    "# ---------------------------\n",
    "# 4) FUNCIÓN PARA CREAR FIGURAS (USAMOS PLOTLY)\n",
    "# ---------------------------\n",
    "\n",
    "def fig_students_by_department(df_summary):\n",
    "    # Barra: estudiantes por departamento\n",
    "    fig = px.bar(\n",
    "        df_summary.sort_values(\"students_count\", ascending=False),\n",
    "        x=\"department\", y=\"students_count\",\n",
    "        title=\"Estudiantes (muestra) por Departamento\",\n",
    "        labels={\"department\":\"Departamento\",\"students_count\":\"Cantidad de estudiantes (en el dataset)\"},\n",
    "        hover_data={\"students_count\":True, \"school_units\":True, \"students_per_school\":\":.2f\"}\n",
    "    )\n",
    "    fig.update_layout(yaxis_title=\"Cantidad de estudiantes\", xaxis_title=\"Departamento\")\n",
    "    # añadir nota explicativa en el layout\n",
    "    fig.add_annotation(\n",
    "        text=\"Nota: 'Cantidad de estudiantes' corresponde a los registros existentes en el archivo cargado.\",\n",
    "        showarrow=False, xref=\"paper\", yref=\"paper\", x=0, y=-0.18, align=\"left\"\n",
    "    )\n",
    "    return fig\n",
    "\n",
    "def fig_students_by_dept_sex(df_grouped):\n",
    "    # Gráfico de barras apiladas por sexo y depto\n",
    "    fig = px.bar(\n",
    "        df_grouped,\n",
    "        x=\"dept_label\", y=\"count\", color=\"sex_label\",\n",
    "        title=\"Estudiantes por Departamento y Sexo\",\n",
    "        labels={\"dept_label\":\"Departamento\",\"count\":\"Cantidad\",\"sex_label\":\"Sexo\"},\n",
    "        barmode=\"stack\",\n",
    "        hover_data={\"count\":True}\n",
    "    )\n",
    "    fig.update_layout(xaxis_title=\"Departamento\", yaxis_title=\"Cantidad de estudiantes\")\n",
    "    fig.add_annotation(\n",
    "        text=\"Stacked bar: permite ver la composición por sexo dentro de cada departamento.\",\n",
    "        showarrow=False, xref=\"paper\", yref=\"paper\", x=0, y=-0.18, align=\"left\"\n",
    "    )\n",
    "    return fig\n",
    "\n",
    "def fig_school_units(dept_summary_df):\n",
    "    # Barras del total de unidades educativas por departamento (datos proporcionados)\n",
    "    fig = px.bar(\n",
    "        dept_summary_df.sort_values(\"school_units\", ascending=False),\n",
    "        x=\"department\", y=\"school_units\",\n",
    "        title=\"Unidades educativas por Departamento (datos oficiales proporcionados)\",\n",
    "        labels={\"department\":\"Departamento\",\"school_units\":\"Unidades educativas\"}\n",
    "    )\n",
    "    fig.update_layout(xaxis_title=\"Departamento\", yaxis_title=\"Unidades educativas\")\n",
    "    fig.add_annotation(\n",
    "        text=\"Estos números son las unidades educativas totales por departamento según tus datos proporcionados.\",\n",
    "        showarrow=False, xref=\"paper\", yref=\"paper\", x=0, y=-0.18, align=\"left\"\n",
    "    )\n",
    "    return fig\n",
    "\n",
    "def fig_students_per_school(dept_summary_df):\n",
    "    # Comparación: estudiantes por escuela (muestra)\n",
    "    fig = px.bar(\n",
    "        dept_summary_df.sort_values(\"students_per_school\", ascending=False),\n",
    "        x=\"department\", y=\"students_per_school\",\n",
    "        title=\"Estudiantes por colegio (muestra / unidades educativas)\",\n",
    "        labels={\"students_per_school\":\"Estudiantes por colegio (promedio en la muestra)\",\"department\":\"Departamento\"},\n",
    "        hover_data={\"students_count\":True,\"school_units\":True}\n",
    "    )\n",
    "    fig.update_layout(xaxis_title=\"Departamento\", yaxis_title=\"Estudiantes por colegio (muestra)\")\n",
    "    fig.add_annotation(\n",
    "        text=\"Cálculo: (estudiantes en el dataset) / (unidades educativas ofrecidas). Útil para tener una idea del volumen promedio por colegio en la muestra.\",\n",
    "        showarrow=False, xref=\"paper\", yref=\"paper\", x=0, y=-0.18, align=\"left\"\n",
    "    )\n",
    "    return fig\n",
    "\n",
    "def fig_institution_type_pie(df_types):\n",
    "    # Pie chart del tipo de institución\n",
    "    fig = px.pie(df_types, names=\"school_type_label\", values=\"count\",\n",
    "                 title=\"Distribución del tipo de institución (pública / privada / convenio)\",\n",
    "                 labels={\"school_type_label\":\"Tipo de institución\",\"count\":\"Cantidad de estudiantes\"})\n",
    "    fig.update_traces(textposition='inside', textinfo='percent+label')\n",
    "    fig.add_annotation(\n",
    "        text=\"Muestra la proporción de estudiantes por tipo de institución en el dataset.\",\n",
    "        showarrow=False, xref=\"paper\", yref=\"paper\", x=0, y=-0.18, align=\"left\"\n",
    "    )\n",
    "    return fig\n",
    "\n",
    "def fig_age_group_hist(df_age):\n",
    "    # Barra de conteo por grupos de edad\n",
    "    fig = px.bar(df_age.sort_values(\"count\", ascending=False),\n",
    "                 x=\"age_group\", y=\"count\", title=\"Distribución por grupo etario\",\n",
    "                 labels={\"age_group\":\"Grupo etario\", \"count\":\"Cantidad de estudiantes (muestra)\"})\n",
    "    fig.update_layout(xaxis_title=\"Grupo etario\", yaxis_title=\"Cantidad de estudiantes\")\n",
    "    fig.add_annotation(\n",
    "        text=\"Grupos definidos: Primera infancia (0-5), Niños/as (6-11), Adolescentes (12-17), Jóvenes (18-28).\",\n",
    "        showarrow=False, xref=\"paper\", yref=\"paper\", x=0, y=-0.18, align=\"left\"\n",
    "    )\n",
    "    return fig\n",
    "\n",
    "def fig_box_age_by_type(df_clean):\n",
    "    # Boxplot de edad por tipo de institución\n",
    "    fig = px.box(df_clean, x=\"school_type_label\", y=\"age_num\", points='outliers',\n",
    "                 title=\"Distribución de edades por tipo de institución\",\n",
    "                 labels={\"school_type_label\":\"Tipo institución\",\"age_num\":\"Edad (años)\"})\n",
    "    fig.update_layout(xaxis_title=\"Tipo de institución\", yaxis_title=\"Edad\")\n",
    "    fig.add_annotation(\n",
    "        text=\"Boxplot: mediana, cuartiles y valores atípicos de las edades por tipo de institución.\",\n",
    "        showarrow=False, xref=\"paper\", yref=\"paper\", x=0, y=-0.18, align=\"left\"\n",
    "    )\n",
    "    return fig\n",
    "\n",
    "# ---------------------------\n",
    "# 5) DASH: LAYOUT Y CALLBACKS\n",
    "# ---------------------------\n",
    "app = Dash(__name__, external_stylesheets=[dbc.themes.FLATLY])\n",
    "server = app.server  # para desplegar si fuera necesario\n",
    "\n",
    "# KPI cards (resumen arriba)\n",
    "kpi_cards = dbc.Row([\n",
    "    dbc.Col(dbc.Card(\n",
    "        [dbc.CardBody([html.H6(\"Total de registros (estudiantes)\", className=\"card-title\"),\n",
    "                       html.H3(f\"{total_students}\", className=\"card-text\"),\n",
    "                       html.P(\"Número total de filas/estudiantes en el dataset cargado.\", className=\"card-subtitle\")])\n",
    "        ]), width=3),\n",
    "    dbc.Col(dbc.Card(\n",
    "        [dbc.CardBody([html.H6(\"Total por sexo\", className=\"card-title\"),\n",
    "                       html.H5(f\"Femenino: {total_by_sex.get('Femenino',0)}\", className=\"card-text\"),\n",
    "                       html.H5(f\"Masculino: {total_by_sex.get('Masculino',0)}\", className=\"card-text\"),\n",
    "                       html.P(\"Conteo por sexo en el dataset.\", className=\"card-subtitle\")])\n",
    "        ]), width=3),\n",
    "    dbc.Col(dbc.Card(\n",
    "        [dbc.CardBody([html.H6(\"Unidades educativas (total conocido)\", className=\"card-title\"),\n",
    "                       html.H3(f\"{total_schools}\", className=\"card-text\"),\n",
    "                       html.P(\"Suma de unidades educativas por departamento según tus datos.\", className=\"card-subtitle\")])\n",
    "        ]), width=3),\n",
    "    dbc.Col(dbc.Card(\n",
    "        [dbc.CardBody([html.H6(\"Promedio: estudiantes / colegio (muestra)\", className=\"card-title\"),\n",
    "                       html.H3(f\"{avg_students_per_school_overall:.2f}\", className=\"card-text\"),\n",
    "                       html.P(\"Total estudiantes del dataset dividido por el total de unidades educativas.\", className=\"card-subtitle\")])\n",
    "        ]), width=3),\n",
    "], className=\"mb-4\")\n",
    "\n",
    "# Layout principal\n",
    "app.layout = dbc.Container([\n",
    "    html.H1(\"Dashboard de Estudiantes - Edades1_filtrado.csv\", className=\"my-3\"),\n",
    "    html.P(\n",
    "        \"Dashboard interactivo que muestra análisis comparativos por departamento, sexo, tipo de institución y grupos etarios. \"\n",
    "        \"Debajo encontrarás KPIs, gráficas comparativas y paneles dinámicos donde elegir métricas y agrupamientos.\",\n",
    "        className=\"lead\"\n",
    "    ),\n",
    "    kpi_cards,\n",
    "\n",
    "    # Primera fila de gráficas comparativas\n",
    "    dbc.Row([\n",
    "        dbc.Col(dcc.Graph(figure=fig_students_by_department(dept_summary)), width=6),\n",
    "        dbc.Col(dcc.Graph(figure=fig_students_by_dept_sex(students_by_dept_sex)), width=6),\n",
    "    ]),\n",
    "\n",
    "    # Segunda fila: unidades educativas y estudiantes por colegio\n",
    "    dbc.Row([\n",
    "        dbc.Col(dcc.Graph(figure=fig_school_units(dept_summary)), width=6),\n",
    "        dbc.Col(dcc.Graph(figure=fig_students_per_school(dept_summary)), width=6),\n",
    "    ]),\n",
    "\n",
    "    # Tercera fila: tipo de institución y distribución por edad\n",
    "    dbc.Row([\n",
    "        dbc.Col(dcc.Graph(figure=fig_institution_type_pie(students_by_school_type)), width=6),\n",
    "        dbc.Col(dcc.Graph(figure=fig_age_group_hist(age_group_counts)), width=6),\n",
    "    ]),\n",
    "\n",
    "    # Cuarta fila: boxplot de edades por tipo\n",
    "    dbc.Row([\n",
    "        dbc.Col(dcc.Graph(figure=fig_box_age_by_type(df_clean)), width=12),\n",
    "    ]),\n",
    "\n",
    "    html.Hr(),\n",
    "\n",
    "    # PANEL DINAMICO 1: elegir métrica y agrupamiento (Grafico 1)\n",
    "    html.H3(\"Panel dinámico 1: elegir métrica y agrupamiento\"),\n",
    "    html.P(\"Selecciona una métrica y un agrupamiento para visualizar estadísticas dinámicas.\"),\n",
    "    dbc.Row([\n",
    "        dbc.Col([\n",
    "            html.Label(\"Métrica:\"),\n",
    "            dcc.Dropdown(\n",
    "                id=\"dyn1_metric\",\n",
    "                options=[\n",
    "                    {\"label\":\"Conteo (nº de estudiantes)\",\"value\":\"count\"},\n",
    "                    {\"label\":\"Edad media\",\"value\":\"mean_age\"},\n",
    "                    {\"label\":\"Edad mediana\",\"value\":\"median_age\"},\n",
    "                    {\"label\":\"Estudiantes por colegio (muestra)\",\"value\":\"students_per_school\"}\n",
    "                ],\n",
    "                value=\"count\"\n",
    "            )\n",
    "        ], width=4),\n",
    "        dbc.Col([\n",
    "            html.Label(\"Agrupar por:\"),\n",
    "            dcc.Dropdown(\n",
    "                id=\"dyn1_groupby\",\n",
    "                options=[\n",
    "                    {\"label\":\"Departamento\",\"value\":\"dept_label\"},\n",
    "                    {\"label\":\"Sexo\",\"value\":\"sex_label\"},\n",
    "                    {\"label\":\"Tipo de institución\",\"value\":\"school_type_label\"},\n",
    "                    {\"label\":\"Grupo etario\",\"value\":\"age_group\"}\n",
    "                ],\n",
    "                value=\"dept_label\"\n",
    "            )\n",
    "        ], width=4),\n",
    "        dbc.Col([\n",
    "            html.Label(\"Tipo de gráfico:\"),\n",
    "            dcc.Dropdown(\n",
    "                id=\"dyn1_charttype\",\n",
    "                options=[\n",
    "                    {\"label\":\"Barra\",\"value\":\"bar\"},\n",
    "                    {\"label\":\"Pie\",\"value\":\"pie\"},\n",
    "                    {\"label\":\"Linea (serie temporal no aplica, es por orden)\",\"value\":\"line\"}\n",
    "                ],\n",
    "                value=\"bar\"\n",
    "            )\n",
    "        ], width=4),\n",
    "    ]),\n",
    "    dbc.Row([\n",
    "        dbc.Col(dcc.Graph(id=\"dyn1_graph\"), width=12)\n",
    "    ]),\n",
    "    html.Br(),\n",
    "\n",
    "    # PANEL DINAMICO 2\n",
    "    html.H3(\"Panel dinámico 2: comparar dos variables\"),\n",
    "    html.P(\"Escoge la variable X (categoría) y la variable de color (subgrupo) para ver la comparación por departamentos u otras variables.\"),\n",
    "    dbc.Row([\n",
    "        dbc.Col([\n",
    "            html.Label(\"Eje X (categoría):\"),\n",
    "            dcc.Dropdown(\n",
    "                id=\"dyn2_x\",\n",
    "                options=[\n",
    "                    {\"label\":\"Departamento\",\"value\":\"dept_label\"},\n",
    "                    {\"label\":\"Tipo institución\",\"value\":\"school_type_label\"},\n",
    "                    {\"label\":\"Grupo etario\",\"value\":\"age_group\"},\n",
    "                    {\"label\":\"Nivel educativo (EDLEV)\",\"value\":\"edlev_label\"}\n",
    "                ],\n",
    "                value=\"dept_label\"\n",
    "            )\n",
    "        ], width=6),\n",
    "        dbc.Col([\n",
    "            html.Label(\"Color / Subgrupo:\"),\n",
    "            dcc.Dropdown(\n",
    "                id=\"dyn2_color\",\n",
    "                options=[\n",
    "                    {\"label\":\"Sexo\",\"value\":\"sex_label\"},\n",
    "                    {\"label\":\"Tipo institución\",\"value\":\"school_type_label\"},\n",
    "                    {\"label\":\"Nivel educativo (EDLEV)\",\"value\":\"edlev_label\"},\n",
    "                    {\"label\":\"Ninguno\",\"value\":None}\n",
    "                ],\n",
    "                value=\"sex_label\"\n",
    "            )\n",
    "        ], width=6),\n",
    "    ]),\n",
    "    dbc.Row([\n",
    "        dbc.Col(dcc.Graph(id=\"dyn2_graph\"), width=12)\n",
    "    ]),\n",
    "\n",
    "    html.Br(),\n",
    "\n",
    "    # PANEL DINAMICO 3: filtro por departamento + comparativa\n",
    "    html.H3(\"Panel dinámico 3: filtrar departamento y comparar métricas\"),\n",
    "    html.P(\"Selecciona uno o varios departamentos y una métrica para comparar.\"),\n",
    "    dbc.Row([\n",
    "        dbc.Col([\n",
    "            html.Label(\"Departamento (selección múltiple):\"),\n",
    "            dcc.Dropdown(\n",
    "                id=\"dyn3_depts\",\n",
    "                options=[{\"label\":d,\"value\":d} for d in sorted(dept_summary[\"department\"].unique())],\n",
    "                value=[d for d in sorted(dept_summary[\"department\"].unique())],\n",
    "                multi=True\n",
    "            )\n",
    "        ], width=6),\n",
    "        dbc.Col([\n",
    "            html.Label(\"Métrica a mostrar:\"),\n",
    "            dcc.Dropdown(\n",
    "                id=\"dyn3_metric\",\n",
    "                options=[\n",
    "                    {\"label\":\"Estudiantes (conteo)\",\"value\":\"students_count\"},\n",
    "                    {\"label\":\"Unidades educativas (school_units)\",\"value\":\"school_units\"},\n",
    "                    {\"label\":\"Estudiantes por colegio (students_per_school)\",\"value\":\"students_per_school\"}\n",
    "                ],\n",
    "                value=\"students_count\"\n",
    "            )\n",
    "        ], width=6),\n",
    "    ]),\n",
    "    dbc.Row([dbc.Col(dcc.Graph(id=\"dyn3_graph\"), width=12)]),\n",
    "\n",
    "    html.Hr(),\n",
    "    # Información y notas al final\n",
    "    html.H4(\"Notas y consideraciones\"),\n",
    "    html.Ul([\n",
    "        html.Li(\"Los conteos y cálculos provienen únicamente del archivo 'Edades1_filtrado.csv' cargado.\"),\n",
    "        html.Li(\"Las 'unidades educativas' por departamento son los valores que nos proporcionaste y se usan para calcular 'estudiantes por colegio (muestra)'.\"),\n",
    "        html.Li(\"Si hay registros con códigos no mapeados aparecen como 'Desconocido' u 'Otro/Desconocido'.\"),\n",
    "        html.Li(\"Si deseas que el dashboard corra dentro de JupyterLab en lugar de como app independiente, coméntame y te doy la variante con JupyterDash.\")\n",
    "    ]),\n",
    "    html.Br(),\n",
    "    html.Div([\n",
    "        html.Small(f\"Resumen de valores faltantes en columnas clave: {missing_summary}\")\n",
    "    ], className=\"mb-4\"),\n",
    "], fluid=True)\n",
    "\n",
    "# ---------------------------\n",
    "# 6) CALLBACKS para paneles dinámicos\n",
    "# ---------------------------\n",
    "@app.callback(\n",
    "    Output(\"dyn1_graph\", \"figure\"),\n",
    "    Input(\"dyn1_metric\", \"value\"),\n",
    "    Input(\"dyn1_groupby\", \"value\"),\n",
    "    Input(\"dyn1_charttype\", \"value\")\n",
    ")\n",
    "def update_dyn1(metric, groupby, charttype):\n",
    "    # Crear un dataframe de resumen según el groupby\n",
    "    if groupby not in df_clean.columns:\n",
    "        # si usuario selecciona dept_label u otro, están en df_clean\n",
    "        pass\n",
    "\n",
    "    if metric == \"count\":\n",
    "        d = df_clean.groupby(groupby).size().reset_index(name=\"value\")\n",
    "    elif metric in [\"mean_age\", \"median_age\"]:\n",
    "        d = df_clean.groupby(groupby)[\"age_num\"].agg(\n",
    "            value=(\"mean\" if metric==\"mean_age\" else \"median\")\n",
    "        ).reset_index()\n",
    "    elif metric == \"students_per_school\":\n",
    "        # necesitamos juntar la info por departamento: students_by_dept / schools_per_department\n",
    "        # si groupby es dept_label, usamos dept_summary directamente:\n",
    "        if groupby == \"dept_label\":\n",
    "            d = dept_summary.rename(columns={\"department\":\"dept_label\"})[[\"dept_label\",\"students_per_school\",\"students_count\",\"school_units\"]].rename(columns={\"students_per_school\":\"value\"})\n",
    "        else:\n",
    "            # para otros groupby, calculamos students por grupo y usamos un divisor aproximado:\n",
    "            # (no hay unidades por cada grupo distinto a departamento), así que mostramos NaN cuando no tiene sentido.\n",
    "            tmp = df_clean.groupby(groupby).size().reset_index(name=\"students_count\")\n",
    "            tmp[\"school_units\"] = np.nan\n",
    "            tmp[\"value\"] = np.nan\n",
    "            d = tmp\n",
    "    else:\n",
    "        d = df_clean.groupby(groupby).size().reset_index(name=\"value\")\n",
    "\n",
    "    # Elegir tipo de gráfico\n",
    "    if charttype == \"bar\":\n",
    "        fig = px.bar(d, x=d.columns[0], y=\"value\", title=f\"{metric} por {groupby}\",\n",
    "                     labels={d.columns[0]:groupby, \"value\":metric})\n",
    "    elif charttype == \"pie\":\n",
    "        # pie requiere valores no nulos\n",
    "        fig = px.pie(d, names=d.columns[0], values=\"value\", title=f\"{metric} por {groupby}\")\n",
    "    else:\n",
    "        fig = px.line(d, x=d.columns[0], y=\"value\", title=f\"{metric} por {groupby}\")\n",
    "\n",
    "    fig.update_layout(margin=dict(t=50, b=100))\n",
    "    return fig\n",
    "\n",
    "@app.callback(\n",
    "    Output(\"dyn2_graph\", \"figure\"),\n",
    "    Input(\"dyn2_x\", \"value\"),\n",
    "    Input(\"dyn2_color\", \"value\")\n",
    ")\n",
    "def update_dyn2(x_col, color_col):\n",
    "    if color_col is None:\n",
    "        # simple count by x_col\n",
    "        d = df_clean.groupby(x_col).size().reset_index(name=\"count\")\n",
    "        fig = px.bar(d, x=x_col, y=\"count\", title=f\"Conteo por {x_col}\")\n",
    "    else:\n",
    "        d = df_clean.groupby([x_col, color_col]).size().reset_index(name=\"count\")\n",
    "        fig = px.bar(d, x=x_col, y=\"count\", color=color_col, barmode=\"group\",\n",
    "                     title=f\"Conteo por {x_col} subdividido por {color_col}\")\n",
    "    fig.update_layout(margin=dict(t=40, b=80))\n",
    "    return fig\n",
    "\n",
    "@app.callback(\n",
    "    Output(\"dyn3_graph\", \"figure\"),\n",
    "    Input(\"dyn3_depts\", \"value\"),\n",
    "    Input(\"dyn3_metric\", \"value\")\n",
    ")\n",
    "def update_dyn3(dept_list_selected, metric):\n",
    "    # Filtrar dept_summary por selección\n",
    "    d = dept_summary[dept_summary[\"department\"].isin(dept_list_selected)]\n",
    "    # Gráfico comparativo\n",
    "    fig = px.bar(d, x=\"department\", y=metric, title=f\"{metric} por departamento (selección)\",\n",
    "                 labels={\"department\":\"Departamento\", metric:metric})\n",
    "    fig.update_layout(margin=dict(t=40, b=80))\n",
    "    return fig\n",
    "\n",
    "# ---------------------------\n",
    "# 7) RUN\n",
    "# ---------------------------\n",
    "if __name__ == \"__main__\":\n",
    "   
    "    app.run(debug=True, port=8050)\n",
    "\n"
   ]
  },
